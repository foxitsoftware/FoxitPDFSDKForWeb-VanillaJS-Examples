{
    "name": "foxit-pdf-sdk-for-web-examples",
    "version": "7.5.0",
    "description": "Foxit pdf sdk for web.",
    "author": "Foxit Software Inc.",
    "main": "./lib/UIExtension.full.js",
    "types": "./lib/UIExtension.d.ts",
    "scripts": {
        "start": "concurrently --kill-others \"npm run start:webpack\" \"npm run start:snapshot-server\"",
        "start:webpack": "webpack serve --config ./build/webpack.config.js --mode development",
        "start:snapshot-server": "node ./node_modules/@foxitsoftware/foxit-pdf-sdk-for-web-library/server/snapshot/src/index -p 3002",
        "build": "webpack --config ./build/webpack.config.js --mode production"
    },
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "^7.12.9",
        "@babel/preset-env": "^7.12.7",
        "babel-loader": "^8.2.2",
        "boxen": "^4.1.0",
        "chalk": "^2.4.1",
        "concurrently": "^5.3.0",
        "copy-webpack-plugin": "^6.3.2",
        "cross-env": "^7.0.2",
        "css-loader": "^5.0.1",
        "fs-extra": "^9.0.1",
        "html-loader": "^1.3.2",
        "html-webpack-plugin": "^4.5.0",
        "http-proxy-middleware": "^0.19.1",
        "koa": "^2.7.0",
        "koa-body": "^4.0.4",
        "koa-body-parser": "^1.1.2",
        "koa-router": "^7.4.0",
        "koa2-connect": "^1.0.2",
        "lru-cache": "^4.1.3",
        "raw-body": "^2.3.3",
        "require-dir": "^1.0.0",
        "rimraf": "^3.0.2",
        "serve-handler": "^6.0.2",
        "style-loader": "^2.0.0",
        "webpack": "^5.9.0",
        "webpack-cli": "^4.2.0",
        "webpack-dev-server": "^3.11.0"
    },
    "serve": {
        "port": 8080,
        "public": "/",
        "proxy": {
            "target": "http://127.0.0.1:3002",
            "changeOrigin": true
        }
    },
    "dependencies": {
        "@foxitsoftware/foxit-pdf-sdk-for-web-library": "^7.5.0"
    }
}
